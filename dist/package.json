{
    "name": "n8n-nodes-smartschool",
    "version": "0.1.5",
    "description": "n8n community node for SmartSchool API",
    "license": "MIT",
    "homepage": "https://github.com/Makcime/n8n-nodes-smartschool",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "workflow",
        "automation"
    ],
    "author": {
        "name": "makcime",
        "email": "makcime@pm.me"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Makcime/n8n-nodes-smartschool.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/SmartSchoolApi.credentials.js",
            "dist/credentials/SmartschoolPortalApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/SmartSchool/SmartSchool.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/node": "^20.17.16",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "dependencies": {
        "fast-xml-parser": "^5.3.0",
        "otplib": "^12.0.1",
        "playwright": "^1.56.1"
    }
}
